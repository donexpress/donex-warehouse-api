{
  "name": "warehouse-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsc-watch --onSuccess \"node dist/index.js\"",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "db:query": "ts-node ./node_modules/typeorm/cli -f ./dist/config/ormconfig.js query",
    "db:migrate": "ts-node ./node_modules/typeorm/cli -f ./dist/config/ormconfig.js migration:run",
    "db:migrate:test": "ts-node ./node_modules/typeorm/cli -f ./dist/config/ormconfig.js -c test migration:run",
    "db:revert": "ts-node ./node_modules/typeorm/cli -f ./dist/config/ormconfig.js migration:revert",
    "db:revert:test": "ts-node ./node_modules/typeorm/cli -f ./dist/config/ormconfig.js -c test migration:revert",
    "db:seed": "node dist/seeds/index.js",
    "db:seed:test": "node dist/seeds/index.js test=true",
    "db:reset": "npm run db:drop && npm run db:migration:run && npm run db:seed",
    "db:reset:test": "npm run db:drop:test && npm run db:migration:run:test && npm run db:seed:test",
    "test": "npm run build && cross-env NODE_ENV=test TS_NODE_FILES=true nyc mocha -r ts-node/register -r source-map-support/register --timeout 30000 --recursive dist/test/index.spec.js dist/test/**/*.spec.js",
    "test:file": "NODE_ENV=test TS_NODE_FILES=true mocha -r ts-node/register -r source-map-support/register --timeout 30000 dist/test/index.spec.js $FILE"
  },
  "keywords": ["warehouse", "donex", "supplier"],
  "author": "Don Express",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "tsc-watch": "^6.0.4",
    "typeorm": "0.3.17"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "cross-var": "^1.1.0",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  },
  "engines": {
    "node": "18.x"
  }
}
